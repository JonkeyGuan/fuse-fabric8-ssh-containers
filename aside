fabric:container-create-ssh --host fusefabric4.gsslab.rdu2.redhat.com --user root --password password --new-user admin --new-user-password admin --resolver manualip --manual-ip fusefabric4.gsslab.rdu2.redhat.com amq-broker2
fabric:container-create-ssh --host fusefabric5.gsslab.rdu2.redhat.com --user root --password password --new-user admin --new-user-password admin --resolver manualip --manual-ip fusefabric5.gsslab.rdu2.redhat.com fuse-services
container-add-profile fuse-services fabric8-amq-producer
container-remove-profile fuse-services fabric8-amq-producer
profile-edit --features mq-client-base mq-client-mattsBrokerNetwork 
