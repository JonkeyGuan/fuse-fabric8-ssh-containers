mq-create --version 1.5 --parent-profile mq-redhat --group redHatBrokerNetwork --kind StandAlone --assign-container amq-broker1 redhat-broker1

profile-display mq-broker-mattsBrokerNetwork.redhat-broker1

profile-edit --pid org.fusesource.mq.fabric.server-redhat-broker1/broker.client.transport.uri=tcp://fusefabric3.gsslab.rdu2.redhat.com:61616

