profile-edit --pid org.fusesource.mq.fabric.server-redhat-broker1/password= mq-broker-mattsBrokerNetwork.redhat-broker1

profile-edit --delete --pid org.fusesource.mq.fabric.server-redhat-broker1/password mq-broker-mattsBrokerNetwork.redhat-broker1

