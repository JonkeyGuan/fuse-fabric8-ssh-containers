profile-edit --pid org.fusesource.mq.fabric.server-redhat-broker1/broker.nob.transport.uri=tcp://fusefabric3.gsslab.rdu2.redhat.com:61617 mq-broker-mattsBrokerNetwork.redhat-broker1

profile-edit --pid org.fusesource.mq.fabric.server-redhat-broker1/broker.network.uri.1=static:(tcp://fusefabric4.gsslab.rdu2.redhat.com:61617)

profile-edit --pid org.fusesource.mq.fabric.server-redhat-broker1/broker.network.uri.1=static:(tcp://fusefabric4.gsslab.rdu2.redhat.com:61617) mq-broker-mattsBrokerNetwork.redhat-broker1

