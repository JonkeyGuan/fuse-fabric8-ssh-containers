profile-edit --pid org.fusesource.mq.fabric.cf/user=admin mq-client-mattsBrokerNetwork

profile-edit --pid org.fusesource.mq.fabric.cf/password=admin mq-client-mattsBrokerNetwork

container-add-profile fuse-services mq-client-mattsBrokerNetwork 
profile-edit --pid org.fusesource.mq.fabric.server-redhat-broker2/broker-name=amq mq-broker-mattsBrokerNetwork.redhat-broker2
profile-edit --pid org.fusesource.mq.fabric.server-redhat-broker1/broker-name=amq mq-broker-mattsBrokerNetwork.redhat-broker1
profile-edit --pid org.fusesource.mq.fabric.cf/brokerUrl=discovery://(fabric:mattsBrokerNetwork) mq-client-mattsBrokerNetwork 
profile-edit --features camel-amq fabric8-amq-producer 
profile-edit --delete --features camel-amq fabric8-amq-producer 
container-add-profile fuse-services fabric8-amq-producer 
profile-edit --pid org.fusesource.mq.fabric.server-redhat-broker2/broker-name=redhat-broker2 mq-broker-mattsBrokerNetwork.redhat-broker2
profile-edit --pid org.fusesource.mq.fabric.server-redhat-broker1/broker-name=redhat-broker1 mq-broker-mattsBrokerNetwork.redhat-broker1



